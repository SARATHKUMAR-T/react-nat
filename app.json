{
  "expo": {
    "name": "react-nat",
    "slug": "react-nat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/streaker.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/streaker-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/streaker.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.saratht.reactnat"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/streaker.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "1021a622-9ac3-4def-b3a2-93429cfeaa65"
      }
    },
    "owner": "saratht"
  }
}
